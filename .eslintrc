{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "plugins": [
      "react"
  ],
  "env": {
    "es6": true,
    "jquery": true
  },
  "rules": {
    "curly": 2,
    "indent": [2, 2, {"SwitchCase": 1}],
    "no-multiple-empty-lines": 2,
    "no-undefined": 2,
    "no-use-before-define": [2, "nofunc"],
    "no-eval": 2,
    "valid-typeof": 2,
    "no-console": 2,
    "no-debugger": 2
  }
}
